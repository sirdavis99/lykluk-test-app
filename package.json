{
  "name": "lykluk",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start -c",
    "prepare": "husky install",
    "prebuild": "npx expo prebuild",
    "pod:update": "cd ios && pod install --repo-update && cd ../",
    "start:dev-client": "npx expo start --dev-client -c",
    "start:develop": "npx expo start",
    "start:staging": "APP_ENV=staging npx expo start -c",
    "start:prod": "npx expo start -c",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "eject": "npx expo eject",
    "reinstall": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && yarn install",
    "install:fix": "npx expo install --fix",
    "doctor": "npx expo-doctor",
    "build:ios-preview": "npx eas build --platform ios --profile preview",
    "build:android-preview": "npx eas build --platform android --profile preview",
    "build:all-preview": "eas build --profile preview --platform all",
    "build:dev": "expo build:android --release-channel development",
    "build:android": "npx eas build --platform android",
    "build:ios": "npx eas build --platform ios",
    "submit:ios": "npx eas submit --platform ios",
    "publish:dev": "eas update --branch development --message 'new updates'",
    "publish:staging": "APP_ENV=staging && eas update --branch staging --message 'staging updates'",
    "publish:preview": "APP_ENV=staging && eas update --branch preview --message 'preview updates'",
    "publish:prod": "NODE_ENV=production && eas update --branch production --message 'go live updates'",
    "build:dev-android": "npx eas build --profile development --platform android",
    "build:dev-ios": "npx eas build --profile development --platform ios",
    "pod:install": "cd ios && pod install && cd ../",
    "gradle:clean": "cd android && ./gradlew clean && cd ../",
    "gradle:assembleRelease": "cd android && ./gradlew assembleRelease",
    "device:create": "eas device:create",
    "device:list": "eas device:list",
    "reset-project": "node ./scripts/reset-project.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "expo": "~51.0.9",
    "expo-asset": "^10.0.6",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.6",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-notifications": "^0.28.6",
    "expo-router": "~3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.15",
    "expo-web-browser": "~13.0.3",
    "formik": "^2.4.6",
    "he": "^1.2.0",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-actions-sheet": "^0.9.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-fade-loading": "^1.0.1",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-otp-textinput": "^1.1.5",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-template-literals": "^7.24.6",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "husky": "^9.0.11",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.1",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  }
}
